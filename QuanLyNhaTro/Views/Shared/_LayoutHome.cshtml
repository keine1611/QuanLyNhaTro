	@*	GioHang*@

	<script>
		//Load Data Gio Hang
		function loadCartModal() {
			$.ajax({
				url: '/ShoppingCart/ModalShoppingCart',
				type: 'post',
				async: false,
				success: function (data) {
					$('#list_itemCart').replaceWith($(data).find('#list_itemCart'));

				}
			})
		}
		//Event Listener click
		$(document).ready(function () {
			$('#link-shoppingcart').click(function () {
				loadCartModal()
				$('#shoppingcart-modal').modal('show')
			})
		});

		$('.btn_addToCart').click(function () {
			$('#hehe').toast('show')
			var id_P = Number($(this).val())
			$.ajax({
				url: '/ShoppingCart/AddToCart',
				type: 'POST',
				data: {
					id_P: id_P,
				},
				success: function (data) {
					if (data.result == 0)
						AddToast('Thêm vào giỏ hàng thành công!')
					else
						if (data.result == 1)
							AddToast('Lỗi khi thêm vào giỏ hàng!')
						else
							AddToast('Sản phẩm đã có trong giỏ hàng!')
				}
			})
		})
	</script>

