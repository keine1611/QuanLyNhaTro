	<!--Script Toast -->
	<script>
		function AddToast(message) {
			$('<div class="toast" data-bs-delay="2000" data-bs-autohide="true" data-bs-animation="true" role="alert" aria-live="assertive" aria-atomic="true"><div class="toast-header"><img style="width:30px" src="https://localhost:44329/Assets/Home/img/icon_notification.png" class="rounded me-2" alt="..."><strong class="me-auto">Thông báo</strong><small class="text-muted"></small><button type="button" class="btn-close" data-bs-dismiss="toast" aria-label="Close"></button></div><div class="toast-body">' + message + '</div></div >').appendTo(".toast-container");
			const extoast = $('.toast').last()
			const toast = new bootstrap.Toast(extoast)
			toast.show()
		}

		function ShowConfirmBox(id_NT) {
			$('#ModalConfirmDelete').find('.btn-delete').attr('id', id_NT)
			const myModal = new bootstrap.Modal($('#ModalConfirmDelete'))
			myModal.show()
		}

		$(document).ready(function () {
			$('.btn-delete').click(function () {
				const myModal = bootstrap.Modal.getInstance($('#ModalConfirmDelete'))
				myModal.hide()
				DeleteNhaTro($('#ModalConfirmDelete').find('.btn-delete').attr('id'))
			})
		})

		function DeleteNhaTro(id_NT) {
			$.ajax({
				url: '/Home/DeleteNhaTro',
				type: 'post',
				data: { id_NT: id_NT },
				async: false,
				success: function (data) {
					if (data == 0) {
						AddToast('Xoá nhà trọ thành công!')
					}
					
					else
						if (data == 1)
							AddToast('Không tìm thấy nhà trọ này!')
						else
							AddToast('Lỗi khi xoá!')
				}
			})
			
		}
	</script>
	<!--End Script Toast -->
